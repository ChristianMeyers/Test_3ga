<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.christian.psytastic"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>Psytastic</name>

  <description>
      Verbl√ºffende Psychologie direkt aus der Uni.
  </description>

  <author href="https://psytastic.de" email="chris@psytastic.de">
      Chris 
  </author>

  <plugin name="cordova-plugin-google-analytics" source="npm" /> <!-- evtl: version="1.7.5" -->
  <plugin name="cordova-plugin-whitelist" source="npm" version="1" />
  <plugin name="onesignal-cordova-plugin" source="npm"/>
  <plugin name="cordova-plugin-firebase" spec="0.1.20" source="npm"/>

  <access origin="*"/>  
  
  <platform name="ios" >
    <preference name="orientation" value="portrait" />
  </platform>
  <platform name="android">
    <preference name="orientation" value="portrait" />
  </platform>


  <preference name="KeepRunning" value="true"/>

  <icon src="images/icon/72.png" density="ldpi" />
  <icon src="images/icon/96.png" density="mdpi" />
  <icon src="images/icon/144.png" density="hdpi" />
  <icon src="images/icon/192.png" density="xhdpi" />
  <icon src="images/icon/512.png" density="xxhdpi" />
  <icon src="images/icon/1024.png" density="xxxhdpi" />

</widget>
